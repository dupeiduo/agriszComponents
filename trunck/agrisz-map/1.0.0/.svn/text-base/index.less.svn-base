.map-ctl {
  position: absolute;
  right: 18px;
  top: 8px;
  z-index: 1;
  span {
    display: block;
    width: 34px;
    height: 34px;
  }
  .center-ctl {
    position: fixed;
    top: 104px;
    display: block;
    background: #72a7fc;
    color: #fff;
    text-align: center;
    border-radius: 4px 4px 0 0;
    i {
      font-size: 30px;
      cursor: pointer;
      line-height: 34px;
      width: 34px;
      height: 34px;
    }
  }
  .layer-ctl {
    display: block;
    cursor: pointer;
    text-align: center;
    border-radius: 4px;
    background: #72a7fc;
      img {
        margin-top: 5px;
      }
  }
}